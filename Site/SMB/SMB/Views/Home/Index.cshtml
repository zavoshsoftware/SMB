		<section id="blog" class="blog blog-carousel">
			<div class="container">
				<div class="row clearfix">
					<div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2">
						<div class="heading heading-2 mb-40 text--center">
							<p class="heading--subtitle">Insights & Articles</p>
							<h2 class="heading--title">Our Latest News</h2>
							<div class="divider--line">
								<i class="divider divider--1"></i>
								<i class="divider divider--2"></i>
								<i class="divider divider--3"></i>
								<i class="divider divider--4"></i>
							</div>
							<p class="heading--desc">Follow our latest news and thoughts which focuses exclusively on design, art, vintage, and also our latest work updates.</p>
						</div>
					</div><!-- .col-md-8 end -->
				</div>
				<div class="row">
					<div class="carousel carousel-dots" data-slide="3" data-slide-res="2" data-autoplay="true" data-nav="false" data-dots="true" data-space="30" data-loop="true" data-speed="800">

					@foreach (var blog in Model.Blogs)
					{
						
						<div class="blog-entry">
							<div class="entry--img">
								<a href="/blog/poat/@blog.UrlParam">
									<img src="@blog.ImageUrl" alt="@blog.Title" />
								</a>
							</div>
							<div class="entry--meta">
								<span>Oct 25, 2016</span>   /
								<span><a href="/blog/@blog.BlogGroup.UrlParam">@blog.BlogGroup.Title</a></span>
							</div>
							<div class="entry--title">
								<h4><a href="/blog/poat/@blog.UrlParam">@blog.Title</a></h4>
							</div>
							<div class="entry--content">
								@blog.Summery
							</div>
							<div class="entry--more">
								<a href="/blog/poat/@blog.UrlParam">read more</a>
							</div>
						</div>
					}
 
					</div>
				</div>
			</div>
		</section>

		<section id="contact1" class="contact contact-1 bg-white pb-90">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-6">
						<div class="row mb-50">
							<div class="col-xs-12 col-sm-12 col-md-12">
								<div class="contact--info">
									<h3>Contact Information</h3>
									<p>Email:<a href="mailto:support@smbcloudsolutions.com.au">support@smbcloudsolutions.com.au</a></p>
									<a class="link--styled" target="_blank" href="https://api.whatsapp.com/send?phone=+61406930553&text=hello">Send Message by whatsapp</a>
								</div>
							</div><!-- .col-md-6 end -->
			 
						</div><!-- .row end -->
				 
					</div><!-- .col-md-6 end -->
					<div class="col-xs-12 col-sm-12 col-md-6">
						<div class="contact-form">
							<div class="mb-0">
								<div class="row">
									<div class="col-md-6">
										<input type="text" class="form-control" name="contact-name" id="name" placeholder="Name" required>
									</div>
									<div class="col-md-6">
										<input type="email" class="form-control" name="contact-email" id="email" placeholder="Email" required>
									</div>
									<div class="col-md-12">
										<textarea class="form-control" name="contact-message" id="message" rows="2" placeholder="Message" required></textarea>
									</div>
									<div class="col-md-12">
										<input type="button" value="Send Message" onclick="submitContactForm();" class="btn btn--secondary btn--block">
									</div>
									<div class="col-md-12 mt-20">

										<p class="alert alert-danger" id="errorDiv"></p>
										<p class="alert alert-success" id="SuccessDiv">Thanks,we received your message sent successfully</p>
									</div>
									</div>
							</div>
						</div>
					</div><!-- .col-md-6 end -->
				</div>
			</div>
		</section>


